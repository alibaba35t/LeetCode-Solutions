//this code was written by Java

int start_index= 0;
int end_index=0;
int result=0;
        
if(s.contains("("))
{
  if(!s.contains(")"))
  {
  return 0;
  }

else
{
start_index = s.indexOf("(");
end_index = s.lastIndexOf(")");
result = (end_index - start_index);
// return result;  ^#Because I considered that result may be negative. If reault is negative we will return 0.
        if(result>0)
        {
            return result;
        }
        else
        {
            return 0; 
        }
}
}

return 0;
